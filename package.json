    "suffix",
    "thousands",
    "twitter",
    "views",
    "youtube"
  ],
  "author": "Gustavo Chavarria",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gustavochavarria/thousands-counter/issues"
  },
  "homepage": "https://github.com/gustavochavarria/thousands-counter#readme",
  "devDependencies": {
    "ava": "^2.4.0",
    "microbundle": "^0.14.2",
    "xo": "^0.25.3"
